{
	"name": "CyberDocGen Dev",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "app",
	"workspaceFolder": "/app",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"vivaxy.vscode-conventional-commits",
				"humao.rest-client"
			],
			"settings": {
				"editor.formatOnSave": true
			}
		}
	},
	"remoteUser": "node"
}
