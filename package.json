  "scripts": {
    "dev": "cross-env DEPLOYMENT_MODE=cloud NODE_ENV=development tsx server/index.ts",
    "build": "vite build && node scripts/build-server.js",
    "start": "cross-env NODE_ENV=production node dist/index.cjs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:coverage:hotspots": "tsx scripts/coverage-hotspots.ts",
    "test:ui": "vitest --ui",
    "test:ui-semantics": "tsx scripts/check-semantic-links.ts",
    "lint": "eslint .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5000 && electron .\"",
    "electron:build": "esbuild electron/main.ts electron/preload.ts --platform=node --bundle --format=esm --packages=external --outdir=dist/electron",
    "test:a11y": "vitest run tests/accessibility",
    "test:size": "npm run build && tsx scripts/check-bundle-size.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "windows:validate": "tsx scripts/validate-wack.ts",
    "build:win": "npm run windows:validate && npm run build && npm run electron:build && electron-builder build --win nsis"
  },
